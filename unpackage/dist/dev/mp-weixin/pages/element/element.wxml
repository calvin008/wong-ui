<view class="content"><scroll-view class="nav-bar" id="nav-bar" scroll-x scroll-with-animation scroll-left="{{scrollLeft}}"><block wx:for="{{tabList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view class="{{['nav-item',(index===tabCurrentIndex)?'current':'']}}" id="{{'tab'+index}}" data-event-opts="{{[['tap',[['changeTab',[index]]]]]}}" bindtap="__e">{{item.name}}</view></block></scroll-view><pull-refresh class="panel-content vue-ref" vue-id="1" top="{{90}}" data-ref="mixPulldownRefresh" data-event-opts="{{[['^refresh',[['onPulldownReresh']]],['^setEnableScroll',[['setEnableScroll']]]]}}" bind:refresh="__e" bind:setEnableScroll="__e" bind:__l="__l" vue-slots="{{['default']}}"><swiper class="swiper-box" id="swiper" duration="{{300}}" current="{{tabCurrentIndex}}" data-event-opts="{{[['change',[['changeTab',['$event']]]]]}}" bindchange="__e"><swiper-item><wong-button vue-id="{{('2')+','+('1')}}" bind:__l="__l"></wong-button></swiper-item><swiper-item>2</swiper-item><swiper-item>3</swiper-item><swiper-item>4</swiper-item><swiper-item>5</swiper-item><swiper-item>6</swiper-item></swiper></pull-refresh></view>